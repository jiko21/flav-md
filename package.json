{
  "name": "flav-md",
  "version": "0.3.4",
  "description": "markdown parser with css style",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "lint": "eslint --fix src/**/*.ts",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:coverage:lcov": "bun test --coverage --coverage-reporter=lcov",
    "test:coverage:all": "bun test --coverage --coverage-reporter=text --coverage-reporter=lcov"
  },
  "keywords": [
    "markdown",
    "css"
  ],
  "author": "jiko21",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jiko21/flav-md.git"
  },
  "homepage": "https://jiko21.me/flav-md/",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@types/node": "24.3.0",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-google": "0.14.0",
    "eslint-config-prettier": "8.10.2",
    "eslint-plugin-prettier": "4.2.1",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  }
}
